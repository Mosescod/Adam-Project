{
    "version": 2,
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/pages/(.*)",
        "dest": "/pages/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }